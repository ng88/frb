DefLang EN


Public Class Main

    Public Class A
    
        Private val As Int := Null
    
        Public Sub Initialize(v As Int)
            val := v
        End Sub
    
        Public Sub printMe()
            Debug.println("A at " + Debug.addressof(Me) + ", val=" + val)
        End Sub
    
    End Class
    
    Public Shared Sub main()
        Dim a As A := Null


        Debug.println("a=" + a)
        
        Dim s As String := "salut "
        Debug.println("s="+s)
        s := s * 4
        Debug.println("s="+s)
        s := s * (1-2)
        Debug.println("s="+s)
        s := s * (2-4)
        Debug.println("s="+s)
        s := s * 0
        Debug.println("s="+s)
        
    End Sub

End Class

