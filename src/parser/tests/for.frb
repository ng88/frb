DefLang EN


Public Class Main

    
    Public Shared Sub main()
        Dim a As Int
        
        For a := 1 To 5 Step 1
            Debug.println("dans for 1 , a vaut " + a)
        Next
        
        For a := 10 DownTo 5 Step 1-2
            Debug.println("dans for 2 , a vaut " + a)
        Next
        
        For a := 1 To 5 Step 2
            Debug.println("dans for 3 , a vaut " + a)
        Next
        
        Dim b As Int
        
        For a := 1 To 3 Step 1
            For b := 1 To 3 Step 1
                Debug.println("dans for/for 4 , a=" + a + " b=" + b)
            Next
        Next
     
    End Sub

End Class

