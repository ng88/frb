Public Class Main

    Public Shared Sub main(a As Int)

	   Debug.showStack()

	   Debug.println("factoriel(" & a & ")=" & Main.fact(a))

    Dim a As Int


	   'Dim b As Bool  := a>5 

    End Sub


    Public Shared Function fact(n As Int) As Int
	   If n > 1 Then
	      Return n * Main.fact(n - 1)
	   Else
	      Return 1
	   End If
    End Function

    Public Shared Sub main()

	   Me.main(5)

    End Sub

End Class


