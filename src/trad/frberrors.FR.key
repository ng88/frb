# error messages definition
# do not change de order, add the new error at the end

@LANG;FR;Francais

UNKNOW;"erreur inconnue"
TOKEN;"mot clef ou identifiant incorrect"
LINE;"ligne"
ERROR;"erreur"
SEMANTIC;"erreur sémantique"
PARSE;"erreur syntaxique"
LEXE;"erreur lexicale"
MISS_BEFORE_ENDL;"mot clef manquant avant la fin de ligne"
ARRAY_INIT;"initialisation du tableau non terminée"
PARENTS_EXPR;"erreur dans une expression parenthésée (...)"
BRACES_EXPR;"erreur dans une expression entre crochet [...]"

NEAR;"près de"

ARITHM;"erreur dans une expression arithmétique"
CLASS_DEC_END;"mauvais terminateur dans la déclaration d'une classe"
FN_DEC_END;"mauvais terminateur dans la déclaration d'une fonction"

FN_RETURN_TYPE;"une fonction doit avoir un type de retour"
SUB_RETURN_TYPE;"une procédure ne doit pas avoir de type de retour"


UNEXPECTED_TOKEN;"token inattendu"

IMPORT;"les instructions import doivent être les premières"
LANG;"jeu de mot clef inexistant"
STRING;"chaine non terminée"





CLASS_NOT_FOUND;"classe non trouvée `%1'"
FN_NOT_FOUND;"fonction non trouvée `%1'"


PARAMARRAY_LAST;"le paramètre ParamTableau peut se trouver seulement comme dernier paramètre"
PARAMARRAY_OPT;"il n'est pas possible de mélanger ParamTableau et paramètres facultatifs"
OPT_PARAM;"tous les paramètres suivants un paramètre facultatif doivent être facultatifs"

DECLARE_INIT_ASSIGN;"l'initialisation doit se faire par référence (avec :=) et non par valeur (avec =)"

UNDECL_ID;"identificateur non déclaré"

REDECL_ID;"identificateur déjà déclaré dans la portée courante"


TYPE_MISMATCH;"type incorrect"


INVALID_LVALUE;"impossible d'affecter une valeur à `%1'"


BAD_RETURN_SUB;"une procédure ne doit pas retourner de valeur"

BAD_RETURN_FUNCTION;"une fonction doit retourner une valeur"

NO_RETURN_FUNCTION;"tous les chemins d'exécution ne contiennent pas une instruction retourne"

FIELD_NOT_FOUND;"champs non trouvé `%1'"

INVALID_OUTSIDE_EXPR;"cette expression est incorrecte en dehors d'une fonction/procédure"

MEMBER_NOT_FOUND;"membre non trouvé `%1'"

FN_AMBIGUITY;"il y a ambiguité sur la fonction `%1', il est impossible de récupérer une référence sur une fonction surchargée"


